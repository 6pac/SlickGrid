{
  "name": "slickgrid",
  "version": "4.0.1",
  "description": "A lightning fast JavaScript grid/spreadsheet",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/browser/index.js",
      "default": "./dist/esm/index.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "types": "dist/types/index.d.ts",
  "main": "dist/browser/index.js",
  "module": "dist/esm/index.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/6pac/SlickGrid.git"
  },
  "keywords": [
    "slickgrid",
    "grid"
  ],
  "author": "Michael Leibman <michael.leibman@gmail.com>",
  "contributors": [
    "Ben McIntyre <email.ben.mcintyre@gmail.com>",
    "Ghislain B."
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/6pac/SlickGrid/issues"
  },
  "homepage": "https://github.com/6pac/SlickGrid#readme",
  "scripts": {
    "cypress": "npx cypress open",
    "cypress:ci": "npx cypress run --reporter xunit --reporter-options output=testresult.xml",
    "predev": "rimraf dist",
    "dev": "node ./scripts/dev-watch.mjs --open",
    "serve": "node ./scripts/dev-watch.mjs --serve",
    "clean": "rimraf dist",
    "lint": "eslint src -c .eslintrc --ext .ts",
    "minify": "node ./scripts/minify.js --start",
    "prebuild:all": "rimraf dist && npm run lint",
    "build:all": "node scripts/builds.mjs --prod && npm run build:types",
    "build:test": "node ./scripts/builds.mjs --test",
    "prebuild:prod": "npm run lint",
    "build:prod": "node ./scripts/builds.mjs --prod",
    "prebuild:types": "npm run lint",
    "build:types": "tsc --emitDeclarationOnly --incremental --declarationMap false --outDir dist/types",
    "prebuild:types:prod": "npm run lint",
    "build:types:prod": "tsc --emitDeclarationOnly --incremental --declarationMap --outDir dist/types",
    "preview:release": "node ./scripts/release.js --create-release github --dry-run --skip-checks",
    "release": "node ./scripts/release.js --create-release github",
    "presass:build": "rimraf dist/styles/css",
    "sass:build": "sass src/styles:dist/styles/css --style=compressed --quiet-deps --no-source-map",
    "postsass:build": "postcss dist/styles/css/**/* --dir dist/styles/css --base dist/styles/css --no-map --use cssnano --use autoprefixer --style=compressed",
    "sass:copy": "copyfiles -f src/styles/*.scss dist/styles/sass",
    "serve:demo": "http-server ./ -p 8080 -a localhost -s"
  },
  "dependencies": {
    "sortablejs": "^1.15.0"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.4",
    "@octokit/rest": "^19.0.13",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "conventional-changelog": "^4.0.0",
    "copyfiles": "^2.4.1",
    "cssnano": "^6.0.1",
    "cssnano-preset-lite": "^3.0.0",
    "cypress": "^9.7.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.17",
    "eslint": "^8.45.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-node": "^11.1.0",
    "fs-extra": "^11.1.1",
    "git-url-parse": "^13.1.0",
    "glob": "^10.3.3",
    "http-server": "^14.1.1",
    "inquirer": "^9.2.8",
    "new-github-release-url": "^1.0.0",
    "path": "^0.12.7",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "semver": "^7.5.4",
    "servor": "^4.0.2",
    "strong-log-transformer": "^2.1.0",
    "typescript": "^5.1.6",
    "uglify-js": "^3.17.4",
    "yargs": "^17.7.2"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 11",
    "not dead"
  ]
}
